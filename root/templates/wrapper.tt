<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="/css/site.min.bd908cc5.css" type="text/css" />
    [% stylesheets() %]


    <script type="text/javascript">
        window.CKEDITOR_BASEPATH = '/js/lib/ckeditor/';
    </script>
    <script src="/js/site.min.7288186c.js"></script>
    [% jsfiles() %]
    <title>Empire Logistics</title>
</head>

<body[% IF c.req.path == '' %] style="overflow: hidden;"[% END %] class="[% template.body_class || body_class %]">
<div id="control-bar">
[% INCLUDE 'topnav.tt' %]
[% FOREACH alert IN c.stash.alert %]
  <div class="alert alert-[% alert.class %]">
    [% alert.message %]
  </div>
[% END %]
[% FOREACH error IN errors %]
  <div class="alert alert-danger">
    [% error %]
  </div>
[% END %]
</div>
[% IF c.req.path == '' %]
    [% content %]
[% ELSE %]
    <div class="container-fluid">
    [% content %]
    </div>
[% END %]
[% c.add_jsfile('/js/refresh-modals.js') %]
</body>
</html>
