        this.target.find('.playlist-thumbs').css('margin-left',0);
    },
    prev: function () {
		// find prev slide
        this.currentIndex = this.getPrevIndex();
        this.advanceTo(this.currentIndex);
    },

    next: function () {
        // find next slide
